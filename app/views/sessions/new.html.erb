<!-- app/views/sessions/new.html.erb -->

<h1>Login</h1>
<%= form_with(url: sessions_path, method: :post) do |form| %>

  <%= form.label :password %>
  <%= form.password_field :password %>

  <%= form.submit "Log in" %>
<% end %>

<%= javascript_include_tag 'timertop', 'data-turbolinks-track': 'reload' %>
